{
  "version": 2,
  "builds": [
    {
      "src": "Sever.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Sever.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "public/uploads/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Sever.js"
    }
  ]
}