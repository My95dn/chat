{
  "version": 2,
  "builds": [
    {
      "src": "Sever.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/main.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/uploads",
      "use": "@vercel/static"
    },
    {
      "src": "views/view.ejs",
      "use": "@vercel/node"
    }
    
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Sever.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "public/uploads/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Sever.js"
    },
    {
      "src": "/api/*",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://chat-eight-wine.vercel.app/",
        "Access-Control-Allow-Methods": "GET, POST",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    }
  ]
}